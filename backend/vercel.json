{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "python3.11",
      "maxDuration": 25
    }
  },
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/predict/diabetes", "dest": "/api/predict.py" },
    { "src": "/chat", "dest": "/api/chat.py" },
    { "src": "/health", "dest": "/api/health.py" }
  ]
}
